Algoritmo "consumo"

Var

     distancia, consumo, media : real

Inicio

         escreval("Distância Percorrida em KM:")
         leia(distancia)
         escreval("Combustível Gasto")
         leia(consumo)
         
         media <- distancia / consumo
         
         escreval("Consumo Médio de ", media:8:3)

Fimalgoritmo